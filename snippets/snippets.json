{
  "FactoryBot - define":{
		"prefix": "fbdef",
		"body": [
			"FactoryBot.define do",
			"\t	$1",
			"end"
		],
		"description": "FactoryBot - define"
	},	
	"FactoryBot - factory": {
		"prefix": "fac",
		"body": [
			"factory :${1:name}${2:${3:, class: ${4:model}}${5:, parent: :${6:factory}}${7:, aliases: [:${8:alias}]}} do",
			"  $0",
			"end"
		],
		"description": "FactoryBot - factory"
	},	
  "FactoryBot - sequence": {   
    "prefix": "seq",   
    "body": [  
        "sequence(:${1:attribute}${2:, initial value}) { |n| ${0:"#{n\}"} }"
